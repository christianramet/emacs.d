alias yum yum -q
alias pwgen-strong pwgen -scn 20 1
alias pwgen-simple pwgen -scn 8 1
alias pwgen-paranoid pwgen -scny 24 1
alias psbymem ps -Ao user,uid,pid,pcpu,pmem,start,time,comm --sort=-pmem
alias psbycpu ps -Ao user,uid,pid,pcpu,pmem,start,time,comm --sort=-pcpu
alias ll ls -lah
alias find-empty-dirs find . -depth -empty -type d
alias ff {find-file $1}
alias dnf dnf -q
alias apt apt -q
